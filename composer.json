{
    "name": "nexxtmove/ai",
    "license": "MIT",
    "require": {
        "illuminate/support": ">=8.0",
        "illuminate/http": ">=8.0",
        "guzzlehttp/guzzle": ">=7.0"
    },
    "require-dev": {
        "orchestra/testbench": ">=6.0",
        "pestphp/pest": "^2.34",
        "laravel/pint": "^1.15"
    },
    "autoload": {
        "psr-4": {
            "Nexxtmove\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nexxtmove\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "pest",
        "lint": "pint"
    }
}
